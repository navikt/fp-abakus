{
    "environment": "prod",
    "kafkaPool": "nav-prod",
    "serviceuserkv": "serviceuser/data/prod/srvfpabakus",
    "minReplicas": "4",
    "maxReplicas": "6",
    "limits": {
        "cpu": "2",
        "mem": "2048Mi"
    },
    "requests": {
        "cpu": "500m",
        "mem": "1024Mi"
    },
    "ingresses": [
        "https://fpabakus.nais.adeo.no",
        "https://fpabakus.intern.nav.no",
        "https://fpabakus.prod-fss-pub.nais.io"
    ],
    "env": {
        "APPD_ENABLED": "true"
    },
    "AZURE_IAC_RULES": [
        {
            "app": "fp-swagger",
            "namespace": "teamforeldrepenger",
            "cluster": "prod-gcp",
            "scopes": [
                "swagger"
            ]
        },
        {
            "app": "fpsak",
            "namespace": "teamforeldrepenger",
            "cluster": "prod-fss"
        },
        {
            "app": "k9-sak",
            "namespace": "k9saksbehandling",
            "cluster": "prod-fss"
        },
        {
            "app": "k9-klage",
            "namespace": "k9saksbehandling",
            "cluster": "prod-fss"
        },
        {
            "app": "k9-formidling",
            "namespace": "k9saksbehandling",
            "cluster": "prod-fss"
        },
        {
            "app": "tiltakspenger-fp",
            "namespace": "tpts",
            "cluster": "prod-fss",
            "roles": [
                "ekstern"
            ]
        }
    ]
}
